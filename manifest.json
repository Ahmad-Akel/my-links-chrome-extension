{
  "manifest_version": 3,
  "name": "Akel Launcher",
  "description": "Quick launch Ahmad Akel Media",
  "version": "1.0.0",
  "icons": {
    "128": "128.png"
  },
  "action": {
    "default_icon": {
      "16": "20.png",
      "48": "48.png",
      "128": "128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "16": "20.png",
      "48": "48.png",
      "128": "128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
